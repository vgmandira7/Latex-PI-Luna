%%%% Leiaute (margens): A (esquerda menor + recuo) ou B (iguais) (^)
\newbool{@LayoutA}
\DeclareOption{Layout=A}{%
  \booltrue{@LayoutA}%
  \ExecuteOptions{%
    Font      = Calibri,%
    RuleWidth = Paper,%
    SectNum   = Off,%
    SectUnnum = Center,%
  }%
}
\DeclareOption{Layout=B}{%
  \boolfalse{@LayoutA}%
  \ExecuteOptions{%
    Font      = Arial,%
    RuleWidth = Zero,%
    SectNum   = On,%
    SectUnnum = Left,%
  }%
}
%%%% Fonte principal: Arial (²), Calibri (¹), Times ou LaTeX
\newbool{@FontArial}
\newbool{@FontCalibri}
\newbool{@FontTimes}
