}
\DeclareOption{Font=Times}{%
  \boolfalse{@FontArial}%
  \boolfalse{@FontCalibri}%
  \booltrue{@FontTimes}%
}
\DeclareOption{Font=LaTeX}{%
  \boolfalse{@FontArial}%
  \boolfalse{@FontCalibri}%
  \boolfalse{@FontTimes}%
}
